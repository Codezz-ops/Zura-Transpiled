const Person := struct {
    name: string;
    age: int;

    const error := enum {
        NAME,
        AGE
    };

    const init := fn(n: string, a: int) Person {
        have p: Person;

        if (n == "" {
            error = NAME;
            return p;
        }

        if (a < 0) {
            error = AGE;
            return p;
        }

        p.name = n;
        p.age = a;

        return p;
    };

    const print := fn() void { 
        print("Name: " + name + ", Age: " + age + "\n");
    };
};

