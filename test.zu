const Person := struct {
    name: string;
    age: int;

    const error := enum {
        NAME,
        AGE
    };

    const print := fn() void {
        print("Name: " + name + ", Age: " + age + "\n");
    };
};

const main := fn() int {
    have p: Person;

    p.name = "John";
    p.age = 30;

    p.print();

    return 0;
};